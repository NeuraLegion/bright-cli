{
  "name": "@neuralegion/nexploit-cli",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/nexploit-cli.git"
  },
  "author": "Artem Derevnjuk <artem.derevnjuk@neuralegion.com>",
  "bin": {
    "nexploit-cli": "./dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/NeuraLegion/nexploit-cli/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "@neuralegion/capture-har": "^0.3.2",
    "find-up": "^4.1.0",
    "js-yaml": "^3.14.0",
    "ms": "^2.1.2",
    "openapi-schemas": "^2.0.4",
    "request": "^2.88.2",
    "request-capture-har": "^1.2.2",
    "request-promise": "^4.2.6",
    "tslib": "^2.0.0",
    "yargonaut": "^1.1.4",
    "yargs": "^15.3.1"
  },
  "description": "NexPloit CLI is a CLI tool that can initialize, stop, polling and maintain scans in NeuraLegions solutions (Such as NexPloit).",
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/chai": "^4.2.11",
    "@types/har-format": "^1.2.4",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "^8.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^10.12.18",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1",
    "terser-webpack-plugin": "^3.0.7",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "2.5.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/NeuraLegion/nexploit-cli#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "keywords": [
    "nexploit",
    "cli",
    "api",
    "nexploit-cli",
    "cyber-security",
    "security",
    "har",
    "oas",
    "crawler",
    "nexmock"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src",
    "format": "prettier --check src",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts",
    "build": "webpack --config webpack.config.js",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run build -- --mode=production"
  }
}
