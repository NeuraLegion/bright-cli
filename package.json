  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/nexploit-cli.git"
  },
  "author": "Artem Derevnjuk <artem.derevnjuk@neuralegion.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NeuraLegion/nexploit-cli/issues"
  },
  "keywords": [
    "nexploit",
    "cli",
    "api",
    "nexploit-cli",
    "cyber-security"
  ],
  "bin": {
    "nexploit-cli": "./dist/index.js"
  },
  "scripts": {
    "lint": "tslint --project . -c tslint.json 'src/**/*.ts' -e node_modules --fix",
    "pretty": "prettier --write 'src/**/*.ts'",
    "format": "tslint --project . -c tslint.json 'src/**/*.ts' -e node_modules --fix",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/*.spec.ts",
    "build": "npm run clean && webpack --config webpack.config.js",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run lint && npm run build -- --mode=production",
    "please": "npm cache clean -f && rm -rf node_modules && rm -f package-lock.json"
  },
  "homepage": "https://github.com/NeuraLegion/nexploit-cli#readme",
  "dependencies": {
    "@types/find-up": "^2.1.1",
    "@types/har-format": "^1.2.4",
    "@types/js-yaml": "^3.12.1",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.44",
    "@types/yargs": "^13.0.0",
    "ajv": "^6.10.2",
    "capture-har": "^1.0.1",
    "find-up": "^3.0.0",
    "js-yaml": "^3.13.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-capture-har": "^1.2.2",
    "request-promise": "^4.2.4",
    "tslib": "1.9.3",
    "yargonaut": "1.1.4",
    "yargs": "12.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.1",
    "tslint-plugin-prettier": "^2.0.1",
    "@types/node": "10.12.18",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-node-externals": "1.7.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "ts-loader": "^5.4.3",
    "chai": "^4.2.0",
    "chalk": "2.4.1",
    "mocha": "^5.2.0",
    "prettier": "^1.18.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5",
    "rimraf": "^2.6.2"
  },
  "description": "NexPloit-CLI is a [CLI](https://en.wikipedia.org/wiki/Command-line_interface) tool that can create HTTP Archive (HAR) files from mock requests generated by  unit-testing and other local automations. These HAR files are required to run  scans using NeuraLegions solutions (Such as NexPloit).  NexPloit-CLI supports the latest Fetch API and provides additional  features to help you generate HAR files during CI/CD flows with ease."
}
