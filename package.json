{
  "name": "@neuralegion/nexploit-cli",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/nexploit-cli.git"
  },
  "author": "Artem Derevnjuk <artem.derevnjuk@neuralegion.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NeuraLegion/nexploit-cli/issues"
  },
  "keywords": [
    "nexploit",
    "cli",
    "api",
    "nexploit-cli",
    "cyber-security",
    "security",
    "har",
    "oas",
    "crawler",
    "nexmock"
  ],
  "bin": {
    "nexploit-cli": "./dist/index.js"
  },
  "scripts": {
    "lint": "tslint --project . -c tslint.json 'src/**/*.ts' -e node_modules",
    "pretty": "prettier --check 'src/**/*.ts'",
    "format": "npm run lint:fix && npm run pretty:fix",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/*.spec.ts",
    "build": "webpack --config webpack.config.js",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run lint && npm run build -- --mode=production"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "tslint -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "https://github.com/NeuraLegion/nexploit-cli#readme",
  "dependencies": {
    "ajv": "^6.12.2",
    "capture-har": "^1.1.0",
    "find-up": "^4.1.0",
    "js-yaml": "^3.13.1",
    "openapi-schemas": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-capture-har": "^1.2.2",
    "request-promise": "^4.2.5",
    "tslib": "^1.11.1",
    "yargonaut": "^1.1.4",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/chai": "^4.2.11",
    "@types/find-up": "^4.0.0",
    "@types/har-format": "^1.2.4",
    "@types/js-yaml": "^3.12.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.12.18",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/yargs": "^15.0.4",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.2",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.1",
    "webpack-node-externals": "1.7.2"
  },
  "description": "NexPloit CLI is a CLI tool that can initialize, stop, polling and maintain scans in NeuraLegions solutions (Such as NexPloit)."
}
