{
  "name": "@neuralegion/nexploit-cli",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/nexploit-cli.git"
  },
  "author": "Artem Derevnjuk <artem.derevnjuk@neuralegion.com>",
  "bin": {
    "nexploit-cli": "./dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/NeuraLegion/nexploit-cli/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=10 <=15"
  },
  "dependencies": {
    "@neuralegion/capture-har": "^0.3.4",
    "@neuralegion/os-service": "^1.0.0",
    "ajv": "^6.12.6",
    "amqplib": "~0.8.0",
    "better-ajv-errors": "^0.7.0",
    "content-type": "^1.0.4",
    "find-up": "^5.0.0",
    "js-yaml": "^4.1.0",
    "ms": "^2.1.3",
    "raw-socket": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "request": "~2.88.2",
    "request-promise": "~4.2.6",
    "semver": "^7.3.5",
    "socks": "~2.6.1",
    "socks-proxy-agent": "~5.0.0",
    "tslib": "^2.3.1",
    "tsyringe": "~4.6.0",
    "win-ca": "^3.4.5",
    "ws": "~8.1.0",
    "yargs": "~16.2.0"
  },
  "description": "NexPloit CLI is a CLI tool that can initialize, stop, polling and maintain scans in NeuraLegions solutions (Such as NexPloit).",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/amqplib": "~0.8.1",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/concat-stream": "^1.6.1",
    "@types/content-type": "^1.1.5",
    "@types/har-format": "^1.2.7",
    "@types/js-yaml": "^4.0.2",
    "@types/mocha": "^9.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^10.17.59",
    "@types/request": "~2.48.7",
    "@types/request-promise": "~4.1.48",
    "@types/semver": "^7.3.8",
    "@types/ws": "^7.4.7",
    "@types/yargs": "~16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "concat-stream": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "pkg": "^5.3.1",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.5",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/NeuraLegion/nexploit-cli#readme",
  "keywords": [
    "nexploit",
    "cli",
    "api",
    "nexploit-cli",
    "cyber-security",
    "security",
    "har",
    "oas",
    "crawler",
    "nexmock"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "./node_modules/@neuralegion/os-service/build/Release/service.node",
      "./node_modules/raw-socket/build/Release/raw.node",
      "./node_modules/win-ca/lib/crypt32-ia32.node",
      "./node_modules/win-ca/lib/crypt32-x64.node"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-windows-x64"
    ],
    "outputPath": "bin",
    "compress": "Brotli"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "cross-env": "cross-env",
    "husky": "husky",
    "lint": "eslint -c .eslintrc.js src",
    "format": "prettier --check src",
    "test:unit": "cross-env TS_NODE_PROJECT=tsconfig.spec.json NODE_ENV=test mocha src/**/*.spec.ts",
    "test:e2e": "cross-env TS_NODE_PROJECT=tests/tsconfig.json NODE_ENV=test mocha tests/**/*.spec.ts",
    "build": "webpack --config webpack.config.js --mode=production",
    "build:pkg": "pkg .",
    "prepare": "is-ci || husky install"
  }
}
