{
  "name": "@neuralegion/nexploit-cli",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/nexploit-cli.git"
  },
  "author": "Artem Derevnjuk <artem.derevnjuk@neuralegion.com>",
  "bin": {
    "nexploit-cli": "./dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/NeuraLegion/nexploit-cli/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@neuralegion/capture-har": "^0.3.2",
    "ajv": "^6.12.4",
    "amqplib": "^0.6.0",
    "better-ajv-errors": "^0.6.7",
    "debug": "^4.1.1",
    "find-up": "^5.0.0",
    "get-port": "^5.1.1",
    "js-yaml": "^3.14.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-router": "^9.4.0",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "ms": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-capture-har": "^1.2.2",
    "request-promise": "^4.2.6",
    "semver": "^7.3.2",
    "socks": "^2.4.4",
    "socks-proxy-agent": "^5.0.0",
    "tslib": "^2.0.1",
    "yargonaut": "^1.1.4",
    "yargs": "^16.0.3"
  },
  "description": "NexPloit CLI is a CLI tool that can initialize, stop, polling and maintain scans in NeuraLegions solutions (Such as NexPloit).",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/chai": "^4.2.12",
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/get-port": "^4.2.0",
    "@types/har-format": "^1.2.4",
    "@types/js-yaml": "^3.12.5",
    "@types/koa": "^2.11.4",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.4.1",
    "@types/koa-send": "^4.1.2",
    "@types/koa-static": "^4.0.1",
    "@types/mocha": "^8.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^10.17.35",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "@types/semver": "^7.3.3",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "pkg": "^4.4.9",
    "prettier": "^2.1.1",
    "semantic-release": "^17.1.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "2.5.2"
  },
  "files": [
    "dist",
    "schemas"
  ],
  "homepage": "https://github.com/NeuraLegion/nexploit-cli#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "keywords": [
    "nexploit",
    "cli",
    "api",
    "nexploit-cli",
    "cyber-security",
    "security",
    "har",
    "oas",
    "crawler",
    "nexmock"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src",
    "format": "prettier --check src",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts",
    "build": "webpack --config webpack.config.js --mode=production",
    "semantic-release": "semantic-release",
    "pack:win": "pkg -o bin/win/nexploit-cli.exe -t node14-windows-x64 -c {CompanyName: 'NeuraLegion', ProductName: 'NexPloit CLI', OriginalFilename: 'nexploit-cli.exe'} dist/index.js",
    "pack:mac": "pkg -o bin/mac/nexploit-cli -t node14-mac-x64 .",
    "pack:linux": "pkg -o bin/linux/nexploit-cli -t node14-linux-x64 ."
  },
  "pkg": {
    "assets": [
      "dist/public/**/*",
      "schemas/**/*"
    ]
  }
}
