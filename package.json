{
  "name": "@brightsec/cli",
  "version": "10.1.0-next.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/bright-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Artem Derevnjuk <artem.derevnjuk@brightsec.com>",
  "bin": {
    "bright-cli": "./dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/NeuraLegion/bright-cli/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=10 <=18"
  },
  "dependencies": {
    "@neuralegion/capture-har": "^0.3.4",
    "@neuralegion/os-service": "^1.0.0",
    "ajv": "^6.12.6",
    "amqplib": "~0.10.2",
    "better-ajv-errors": "^1.2.0",
    "chalk": "^4.1.2",
    "content-type": "^1.0.4",
    "find-up": "^5.0.0",
    "js-yaml": "^4.1.0",
    "ms": "^2.1.3",
    "raw-socket": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "request": "~2.88.2",
    "request-promise": "~4.2.6",
    "semver": "^7.3.5",
    "socks": "~2.6.1",
    "socks-proxy-agent": "~5.0.0",
    "tslib": "^2.3.1",
    "tsyringe": "~4.6.0",
    "win-ca": "^3.5.0",
    "ws": "~8.1.0",
    "yargs": "~16.2.0"
  },
  "description": "Bright CLI is a CLI tool that can initialize, stop, polling and maintain scans in Bright solutions.",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/amqplib": "~0.8.2",
    "@types/concat-stream": "^1.6.1",
    "@types/content-type": "^1.1.5",
    "@types/har-format": "^1.2.7",
    "@types/jest": "^28.1.7",
    "@types/js-yaml": "^4.0.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^10.17.59",
    "@types/request": "~2.48.7",
    "@types/request-promise": "~4.1.48",
    "@types/semver": "^7.3.8",
    "@types/ws": "^7.4.7",
    "@types/yargs": "~16.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "clean-webpack-plugin": "^4.0.0",
    "concat-stream": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "nodemon": "^2.0.19",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "terser-webpack-plugin": "^5.3.5",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/NeuraLegion/bright-cli#readme",
  "keywords": [
    "nexploit",
    "cli",
    "api",
    "bright",
    "cyber-security",
    "security",
    "har",
    "oas",
    "crawler",
    "nexmock"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "./node_modules/@neuralegion/os-service/build/Release/service.node",
      "./node_modules/raw-socket/build/Release/raw.node",
      "./node_modules/win-ca/lib/crypt32-ia32.node",
      "./node_modules/win-ca/lib/crypt32-x64.node"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-windows-x64"
    ],
    "outputPath": "bin",
    "compress": "Brotli"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "cross-env": "cross-env",
    "husky": "husky",
    "eslint": "eslint",
    "prettier": "prettier",
    "webpack": "webpack",
    "pkg": "pkg",
    "lint": "eslint -c .eslintrc.js src tests",
    "format": "prettier --check src tests",
    "test:unit": "cross-env NODE_ENV=test jest src",
    "test:e2e": "cross-env NODE_ENV=test jest tests",
    "build": "webpack --config webpack.config.js --mode=production",
    "build:pkg": "pkg .",
    "prepare": "is-ci || husky install",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts"
  }
}
